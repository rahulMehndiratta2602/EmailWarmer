# API Settings
API_V1_STR=/api/v1
PROJECT_NAME=Email Warmup Automation

# Database Settings
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/email_warmup
MONGODB_URL=mongodb://localhost:27017/email_warmup

# Redis Settings
REDIS_URL=redis://localhost:6379/0

# Security
SECRET_KEY=your-secret-key-here
ACCESS_TOKEN_EXPIRE_MINUTES=11520  # 8 days

# Proxy Settings
PROXY_API_KEY=your-proxy-api-key-here
PROXY_SERVICE=brightdata  # brightdata, smartproxy, tor

# Email Provider Settings
EMAIL_PROVIDERS=gmail,outlook,yahoo
MAX_ACCOUNTS_PER_WORKER=100

# Task Settings
TASK_RETRY_DELAY=300  # 5 minutes
MAX_TASK_RETRIES=3

# Monitoring
MONITORING_INTERVAL=60  # seconds
ALERT_THRESHOLD=0.1  # 10% failure rate

# CAPTCHA Service
CAPTCHA_API_KEY=your-captcha-api-key-here
CAPTCHA_SERVICE=2captcha  # 2captcha, anti-captcha

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s 